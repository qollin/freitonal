<ui:UiBinder
 xmlns:ui='urn:ui:com.google.gwt.uibinder'
 xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 
 <ui:style>
 	.overlay {
	    position: relative;
    	top: -100%;
    	right: -10%;
    	z-index: 5;
    }
 </ui:style>
   
  <g:HTMLPanel>
  	<g:ListBox ui:field="list"/>
	<g:Label ui:field="label"/>
	<g:Image ui:field="closeImage" url="X.png" width="12px" height="12px" addStyleNames='{style.overlay}'/>
  </g:HTMLPanel>
</ui:UiBinder>