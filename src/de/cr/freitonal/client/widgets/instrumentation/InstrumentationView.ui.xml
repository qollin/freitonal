<ui:UiBinder
 xmlns:ui='urn:ui:com.google.gwt.uibinder'
 xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  
   <ui:style>
 	.fieldButton {
		float: right;
     }
  </ui:style>
  
  <g:HTMLPanel>
    <div class="fieldWrapper">
      <div class="fieldLabel">
        <g:Label ui:field="label"/>
      </div>
      <div class="fieldContents">
	      <div id="piece-instrumentation-instruments" class="fieldLabel"/>
	      <g:Button ui:field="addInstrumentButton" addStyleNames='{style.fieldButton}'>+</g:Button>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>